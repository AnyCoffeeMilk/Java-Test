package com.ikl.test;

public class Dog {
	private int size;
	private int age;
	private String name;
	
	public void setName(String name) {
		this.name = name;
	}
	public void setAge(int age) {
		this.age = age;
	}
	public void setSize(int size) {
		this.size = size;
	}
	public void getName() {
		System.out.println(this.name);
	}
	
	public static void main(String arg[]) {
		Dog dog = new Dog();
		dog.setName("Puppy");
	}
}
